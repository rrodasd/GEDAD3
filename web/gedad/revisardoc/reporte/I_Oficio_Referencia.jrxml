	<queryString>
		<![CDATA[SELECT	REO.VREFORD_DESC,CLA.VCLASE_NOM_CORTO,DOC.CDOCUMENTO_NRO_ORDEN,DOC.VDOCUMENTO_CLAVE_INDIC,
	TO_CHAR(DDOCUMENTO_FECHA,'DD-MON-YY') AS DDOCUMENTO_FECHA
FROM	EP_GEDAD.SAGDE_REFERENCIA REF
	INNER JOIN EP_GEDAD.SAGDE_REFERENCIA_ORDEN REO ON 	REF.VREFORD_COD = REO.VREFORD_COD
	INNER JOIN EP_GEDAD.SAGDE_DOCUMENTO DOC ON 	REF.CREFERENCIA_PERIODO_REF = DOC.CDOCUMENTO_PERIODO AND
						REF.CREFERENCIA_COD_DOC_REF = DOC.CDOCUMENTO_COD_DOC_INT
	INNER JOIN EP_GEDAD.SAGDE_CLASE CLA ON DOC.CDOCUMENTO_CLASE = CLA.CCLASE_CODIGO
WHERE	REF.CREFERENCIA_PERIODO_ORIG = $P{PERIODO} AND
	REF.CREFERENCIA_COD_DOC_ORIG = $P{CODIGO_GEDAD}
ORDER BY	REO.VREFORD_COD ASC]]>
	</queryString>
